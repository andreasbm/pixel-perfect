{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "pixel-perfect",
    "ejected": true
  },
  "apps": [
    {
      "name": "pixel-perfect",
      "root": "src/lib",
      "outDir": "../../dist/lib",
      "test": "test.ts",
      "main": "index.ts",
      "index": "../demo/index.html",
      "prefix": "fp",
      "tsconfig": "./../../tsconfig.lib.spec.json"
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
