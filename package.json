{
	"name": "pixel-perfect-srcs",
	"version": "1.0.0",
	"description": "A pixel perfect SCSS stylesheet. Spiced with themeable mixins, custom CSS variables and other cool stuff you can handpick from.",
	"author": "Appnest",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andreasbm/pixel-perfect/issues"
	},
	"homepage": "https://github.com/andreasbm/pixel-perfect#readme",
	"main": "index.js",
	"jsnext:main": "index.js",
	"module": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"clean": "rimraf dist/*",
		"b:lib": "npm run clean; node build.js",
		"b:demo": "ng build --prod",
		"start": "ng serve -o --port=1231",
		"update": "ncu -ua; npm update",
		"bump:patch": "cd src/lib && npm version patch && cd ../..;",
		"bump:minor": "cd src/lib && npm version minor && cd ../..;",
		"bump:major": "cd src/lib && npm version major && cd ../..;",
		"publish:patch": "npm run bump:patch; npm run b:lib; cd dist; npm publish && cd ..",
		"publish:minor": "npm run bump:minor; npm run b:lib; cd dist; npm publish && cd ..",
		"publish:major": "npm run bump:major; npm run b:lib; cd dist; npm publish && cd ..",
		"deploy": "npm run b:demo; firebase deploy"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andreasbm/pixel-perfect.git"
	},
	"keywords": [
		"pixel",
		"perfect",
		"stylesheet",
		"scss",
		"simple",
		"css",
		"styles"
	],
	"dependencies": {
		"@angular/common": "^5.2.9",
		"@angular/compiler": "^5.2.9",
		"@angular/core": "^5.2.9",
		"@angular/forms": "^5.2.9",
		"@angular/http": "^5.2.9",
		"@angular/platform-browser": "^5.2.9",
		"@angular/platform-browser-dynamic": "^5.2.9",
		"@angular/platform-server": "^5.2.9",
		"@angular/router": "^5.2.9",
		"core-js": "^2.5.3",
		"rxjs": "^5.5.7",
		"zone.js": "^0.8.20"
	},
	"devDependencies": {
		"@angular/cli": "1.7.3",
		"@angular/compiler-cli": "^5.2.9",
		"@types/jasmine": "2.8.6",
		"@types/node": "~9.4.7",
		"autoprefixer": "^8.1.0",
		"codelyzer": "~4.2.1",
		"css-loader": "^0.28.11",
		"cssnano": "^3.10.0",
		"exports-loader": "^0.7.0",
		"file-loader": "^1.1.11",
		"istanbul-instrumenter-loader": "^3.0.0",
		"jasmine-core": "~3.1.0",
		"jasmine-spec-reporter": "~4.2.1",
		"json-loader": "^0.5.7",
		"karma": "~2.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.4.2",
		"karma-jasmine": "~1.1.1",
		"karma-jasmine-html-reporter": "^1.0.0",
		"karma-sourcemap-loader": "^0.3.7",
		"less-loader": "^4.1.0",
		"mkdirp": "^0.5.1",
		"ncp": "^2.0.0",
		"postcss-loader": "^2.1.2",
		"protractor": "~5.3.0",
		"raw-loader": "^0.5.1",
		"rimraf": "^2.6.2",
		"node-sass": "^4.7.2",
		"postcss": "^6.0.20",
		"postcss-scss": "^1.0.4",
		"postcss-url": "7.3.1",
		"rollup": "0.57.1",
		"rollup-plugin-sass": "^0.6.0",
		"rollup-plugin-uglify": "^3.0.0",
		"glob": "^7.1.2",
		"postcss-sassy-import": "^1.2.3",
		"postcss-discard-comments": "^2.0.4",
		"sass-loader": "^6.0.7",
		"script-loader": "^0.7.2",
		"source-map-loader": "^0.2.3",
		"style-loader": "^0.20.3",
		"stylus-loader": "^3.0.2",
		"ts-node": "~5.0.1",
		"tslint": "~5.9.1",
		"typescript": "~2.7.2",
		"url-loader": "^1.0.1",
		"webpack-dev-server": "~3.1.1"
	}
}
